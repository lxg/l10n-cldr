{
    "name": "@lxg/l10n-cldr",
    "version": "0.4.0",
    "description": "Provides CLDR data as CommonJS and ES modules",
    "type": "module",
    "scripts": {
        "lint": "for script in $(find src/ -iname '*.js'); do npx eslint $script; done",
        "lint:fix": "for script in $(find src/ -iname '*.js'); do npx eslint $script --fix; done",
        "build": "test -d ./dist && rm -r ./dist; mkdir -p ./dist/esm && ./bin/generate.js && cp ./src/esm/* ./dist/esm/"
    },
    "main": "./dist/esm/cldr.js",
    "exports": {
        ".": {
            "import": "./dist/esm/cldr.js"
        },
        "./": {
            "require": "./dist/cjs/data/",
            "import": "./dist/esm/data/"
        }
    },
    "files": [
        "dist/*"
    ],
    "keywords": [
        "internationalisation",
        "i18n",
        "localization",
        "l10n",
        "cldr",
        "unicode",
        "translation",
        "translate",
        "localize"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lxg/l10n-cldr.git"
    },
    "author": {
        "name": "Alexander GÃ¼nsche",
        "url": "https://lxg.de/"
    },
    "license": "ISC",
    "devDependencies": {
        "cldr-core": "^39.0.0",
        "cldr-dates-modern": "^39.0.0",
        "cldr-localenames-full": "^39.0.0",
        "fast-glob": "^3.2.4",
        "rimraf": "^3.0.2",
        "eslint": "^7.32.0"
    }
}
